{
    "name": "docker-cmd",
    "version": "0.1.0",
    "description": "A Docker NodeJS lib wrapping the Docker command line",
    "keywords": ["docker"],
    "homepage": "http://github.com/iorga-group/docker-cmd",
    "license": "Apache-2.0",
    "author": "Anthony Ogier",
    "main": "index.js",
    "dependencies": {
        "commander": "~2.3.0",
        "extend": "~1.3.0"
    },
    "devDependencies": {
        "mocha": "~1.21.4",
        "should": "~4.0.4",
        "sinon": "~1.10.3",
        "mock-spawn": "~0.2.1",
        "blanket": "~1.1.6",
        "travis-cov": "~0.2.5",
        "mocha-multi": "~0.4.1"
    },
    "config": {
        "blanket": {
            "pattern": "lib/docker"
        }
    },
    "scripts": {
        "test": "multi='travis-cov=- dot=-' ./node_modules/.bin/mocha --require blanket -R mocha-multi",
        "test-html-cov-report": "./node_modules/.bin/mocha --require blanket -R html-cov > coverage.html"
    },
    "bin": {
        "docker-cm": "./bin/docker-cm.js"
    }
}
